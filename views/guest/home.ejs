<!DOCTYPE html>
<html>
<head>
    <title>My Blog</title>
</head>
<body>
    <h1>Welcome to My Blog</h1>
    <div class="articles">
        <% articles.forEach(article => { %>
            <div class="article">
                <h2><a href="/guest/article/<%= article.id %>"><%= article.title %></a></h2>
                <p>Published on: <%= article.publishDate.toLocaleDateString() %></p>
            </div>
        <% }); %>
    </div>
</body>
</html>
